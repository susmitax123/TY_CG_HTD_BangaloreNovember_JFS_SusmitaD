package com.capgemini.forestryjdbcproject.factory;

import com.capgemini.jdbcproject.dao.ContractorDAO;
import com.capgemini.jdbcproject.dao.ContractorDAOImpl;
import com.capgemini.jdbcproject.services.ContractorService;
import com.capgemini.jdbcproject.services.ContractorServiceImpl;

public class ContractorFactory {
public ContractorFactory() {
	// TODO Auto-generated constructor stub
}

public static ContractorDAO instanceOfContractorDAOImpl() {
	ContractorDAO dao = new ContractorDAOImpl();
	return dao;
}

public static ContractorService intanceOContractorServiceImpl() {
	ContractorService service = new ContractorServiceImpl();
	return service;
}
}
