package com.capgemini.forestryapp;

import java.util.Scanner;

public class Admin {

	public Admin() {
		System.out.println("Welcome to Admin Page!");
		System.out.println();
		Scanner sc = new Scanner(System.in);
		System.out.println("1.To deal with Contractor");
		System.out.println("2.To deal with Customer");
		System.out.println("3.To deal with Land");
		System.out.println("4.To deal with Product");
		int choice = sc.nextInt();
		switch (choice) {
		case 1:
			Contract contractor = new Contract(); 
			break;

		default:
			break;
		}
			
	}
}
